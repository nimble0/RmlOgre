// Full window dark blue background with 1px red border all around
// 50px yellow square in top left
// 50px magenta square in top right
// 50px cyan square in bottom left
// 50px blue square in bottom right
<rml>
	<head>
		<style>
			body
			{
				width: 100%;
				height: 100%;
				background-color: #005;
				border: 1px #f00;
			}

			#top-left,
			#top-right,
			#bottom-left,
			#bottom-right
			{
				position: absolute;
				width: 50px;
				height: 50px;
			}

			#top-left
			{
				top: 0;
				left: 0;
				background-color: #ff0;
			}

			#top-right
			{
				top: 0;
				right: 0;
				background-color: #f0f;
			}

			#bottom-left
			{
				bottom: 0;
				left: 0;
				background-color: #0ff;
			}

			#bottom-right
			{
				bottom: 0;
				right: 0;
				background-color: #00f;
			}
		</style>
	</head>
	<body>
		<div id="top-left" />
		<div id="top-right" />
		<div id="bottom-left" />
		<div id="bottom-right" />
	</body>
</rml>
