compositor_node Start
{
	texture rt0 target_width target_height PFG_RGBA16_FLOAT msaa_auto
	texture rt1 target_width target_height PFG_RGBA16_FLOAT msaa_auto

	target rt0
	{
		pass clear
		{
			colour_value 0 0 0 0
		}
	}

	target rt1
	{
		pass clear
		{
			colour_value 0 0 0 0
		}
	}

	out 0 rt0
	out 1 rt1
}

compositor_node End
{
	in 0 rt0
	in 1 rt1

	target rt1
	{
		pass render_quad
		{
			material Ogre/Copy/4xFP32
			input 0 rt0
		}
	}
}
